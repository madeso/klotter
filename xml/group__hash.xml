</parameterdescription>
</parameteritem>
</parameterlist>
</para>
        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
        <location file="src/klotter/hash.h" line="50" column="9" bodyfile="src/klotter/hash.h" bodystart="50" bodyend="58"/>
